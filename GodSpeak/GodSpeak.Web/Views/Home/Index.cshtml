
<!-- hero map -->
<section class="map-wrapper">
    <div class="" id="map"></div>
    <div class="container" style="position: relative;">
        <div id="share-panel" class="panel panel-default panel-share">
            <div class="panel-heading">
                <h2 class="panel-title">Share your impact</h2>
            </div>
            <div class="panel-body">
                <label for="share-link">Share via Link:</label>
                <form>
                    <div class="input-group">

                        <input type="text"
                               class="form-control"
                               id="share-link"
                               aria-describedby="basic-addon3"
                               value="http://impact.givegodspeak.com/">
                        <span class="input-group-btn">
                            <button class="btn btn-default btn-copy"
                                    data-clipboard-target="#share-link"
                                    data-toggle="popover"
                                    data-placement="bottom"
                                    data-content="Copied!"
                                    type="button">
                                <span class="sr-only">Copy to Clipboard</span>
                            </button>
                        </span>
                    </div>

                    <label for="embed-link">Embed Map in Your Site:</label>
                    <div class="input-group">

                        <input type="text"
                               class="form-control"
                               id="embed-link"
                               aria-describedby="basic-addon3"
                               value="<iframe src=‘http://impact.give'></iframe>">
                        <span class="input-group-btn">
                            <button class="btn btn-default btn-copy"
                                    data-clipboard-target="#embed-link"
                                    data-toggle="popover"
                                    data-placement="bottom"
                                    data-content="Copied!"
                                    type="button">
                                <span class="sr-only">Copy to Clipboard</span>
                            </button>
                        </span>
                    </div>
                </form>
            </div>
        </div>
    </div>

</section>
